{
  "swagger": "2.0",
  "info": {
    "title": "ext_transaction.proto",
    "version": "version not set"
  },
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/api/transactions/history": {
      "get": {
        "operationId": "TransactionHistory",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/extapiTransactionHistoryResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/runtimeError"
            }
          }
        },
        "parameters": [
          {
            "name": "currency",
            "description": "currency.",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "from",
            "description": "period for which transactions should be returned.",
            "in": "query",
            "required": false,
            "type": "string",
            "format": "date-time"
          },
          {
            "name": "till",
            "in": "query",
            "required": false,
            "type": "string",
            "format": "date-time"
          },
          {
            "name": "paymentType",
            "description": "if non-empty list of payment types is given only transactions of these\ntypes will be returned.",
            "in": "query",
            "required": false,
            "type": "array",
            "items": {
              "type": "string"
            },
            "collectionFormat": "multi"
          },
          {
            "name": "offset",
            "in": "query",
            "required": false,
            "type": "string",
            "format": "int64"
          },
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "type": "string",
            "format": "int64"
          }
        ],
        "tags": [
          "TransactionService"
        ]
      }
    },
    "/api/transactions/withdraw-fee": {
      "get": {
        "operationId": "GetWithdrawFee",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/extapiGetWithdrawFeeResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/runtimeError"
            }
          }
        },
        "parameters": [
          {
            "name": "currency",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "amount",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "TransactionService"
        ]
      }
    }
  },
  "definitions": {
    "extapiGetWithdrawFeeResponse": {
      "type": "object",
      "properties": {
        "totalFee": {
          "type": "string"
        },
        "transactionFee": {
          "type": "string"
        },
        "communityContribution": {
          "type": "string"
        },
        "feeCurrency": {
          "type": "string"
        }
      }
    },
    "extapiTransaction": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "format": "int64"
        },
        "timestamp": {
          "type": "string",
          "format": "date-time"
        },
        "amount": {
          "type": "string",
          "title": "amount, decimal number with up to 18 decimal digits"
        },
        "paymentType": {
          "type": "string",
          "title": "payment type as a string"
        },
        "detailsJson": {
          "type": "string",
          "title": "transaction details encoded as JSON"
        }
      }
    },
    "extapiTransactionHistoryResponse": {
      "type": "object",
      "properties": {
        "tx": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/extapiTransaction"
          }
        }
      }
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "typeUrl": {
          "type": "string"
        },
        "value": {
          "type": "string",
          "format": "byte"
        }
      }
    },
    "runtimeError": {
      "type": "object",
      "properties": {
        "error": {
          "type": "string"
        },
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    }
  }
}
